.music-head{
	width: 100%;
	height: 44px;
	position: relative;
	text-align: center;
	line-height: 44px;
}
.music-head-icon{
	width: 90px;
	height: 28px;
	border: 1px solid #ccc;
	/* 形成弹性盒 */
	display: flex;
	/* 弹性盒中默认的主轴为x轴 设置所有的空间都是自动分配的 */
	justify-content: space-evenly;
	/* 侧轴是y轴 侧轴对齐的 */
	align-items: center;
	position: absolute;
	left:8px ;
	top: 7px;
	border-radius: 20px;
}
.music-head-icon ._span{
	width: 1px;
	height: 17px;
	background: #ededed;
}

